@model List<Book>

@{
    ViewBag.Title = "Home Page";

}

<main>
     @Html.Kendo().DatePicker().Name("VotingDate");
     
    <h1>Welcome Bacl</h1>
    <div id="messagebox"></div>
   @if (Model != null)
   {
       <h1>Books</h1>
       foreach (var book in Model)
       {
             <div>
                 @book.Title - @book.Author | <a href="/rental?id=@book.Id">Rent</a>
             </div>

       }
   }

</main>